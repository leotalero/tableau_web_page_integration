package co.sistemcobro.rrhh.bean;

import java.io.Serializable;
import java.sql.Timestamp;

/**
 * 
 * @author Jony Hurtado
 * 
 */
public class CargoAreaAsignada implements Serializable {

	private static final long serialVersionUID = 1L;
	
	private Long idcargo;
	private Long idareaasignada;
	private Long idarea;
	private String  nombrearea;

	private Integer idusuariocrea;
	private Timestamp fechacrea;
	private Integer idusuariomod;
	private Timestamp fechamod;
	private Integer estado;

	
	private Cargo cargo;
	private Area areaasignada;
	
	
	
	public Long getIdcargo() {
		return idcargo;
	}
	public void setIdcargo(Long idcargo) {
		this.idcargo = idcargo;
	}
	public Long getIdareaasignada() {
		return idareaasignada;
	}
	public void setIdareaasignada(Long idareaasignada) {
		this.idareaasignada = idareaasignada;
	}

	
	public Long getIdarea() {
		return idarea;
	}
	public void setIdarea(Long idarea) {
		this.idarea = idarea;
	}
	public String getNombrearea() {
		return nombrearea;
	}
	public void setNombrearea(String nombrearea) {
		this.nombrearea = nombrearea;
	}
	public Integer getIdusuariocrea() {
		return idusuariocrea;
	}
	public void setIdusuariocrea(Integer idusuariocrea) {
		this.idusuariocrea = idusuariocrea;
	}
	public Timestamp getFechacrea() {
		return fechacrea;
	}
	public void setFechacrea(Timestamp fechacrea) {
		this.fechacrea = fechacrea;
	}
	public Integer getIdusuariomod() {
		return idusuariomod;
	}
	public void setIdusuariomod(Integer idusuariomod) {
		this.idusuariomod = idusuariomod;
	}
	public Timestamp getFechamod() {
		return fechamod;
	}
	public void setFechamod(Timestamp fechamod) {
		this.fechamod = fechamod;
	}
	public Integer getEstado() {
		return estado;
	}
	public void setEstado(Integer estado) {
		this.estado = estado;
	}
	public Cargo getCargo() {
		return cargo;
	}
	public void setCargo(Cargo cargo) {
		this.cargo = cargo;
	}
	public Area getAreaasignada() {
		return areaasignada;
	}
	public void setAreaasignada(Area areaasignada) {
		this.areaasignada = areaasignada;
	}
	
	
	

	


}
