package co.sistemcobro.rrhh.bean;

import java.io.Serializable;
import java.sql.Timestamp;
import java.util.List;

import co.sistemcobro.asistencia.bean.Asistencia;

/**
 * 
 * @author Leonardo talero
 * 
 */
public class ReporteAsignacion implements Serializable {

	private static final long serialVersionUID = 1L;
	
	private Asistencia asistencia;
	private EmpleadoBean empleado;
	private Contrato contrato;
	private IdentificacionTipo identificaciontipo;
	private EmpleadoIdentificacion empleadoidentificacion;
	private Cargo cargo;
	private Area area;
	private HorarioAsignado horarioasignado;
	private Timestamp horaentradadiaria;
	private Timestamp horasalidadiaria;
	private  List<HorarioparaReporte>   valores;
	
	public Asistencia getAsistencia() {
		return asistencia;
	}
	public void setAsistencia(Asistencia asistencia) {
		this.asistencia = asistencia;
	}
	public EmpleadoBean getEmpleado() {
		return empleado;
	}
	public void setEmpleado(EmpleadoBean empleado) {
		this.empleado = empleado;
	}
	public Contrato getContrato() {
		return contrato;
	}
	public void setContrato(Contrato contrato) {
		this.contrato = contrato;
	}
	public IdentificacionTipo getIdentificaciontipo() {
		return identificaciontipo;
	}
	public void setIdentificaciontipo(IdentificacionTipo identificaciontipo) {
		this.identificaciontipo = identificaciontipo;
	}
	public EmpleadoIdentificacion getEmpleadoidentificacion() {
		return empleadoidentificacion;
	}
	public void setEmpleadoidentificacion(
			EmpleadoIdentificacion empleadoidentificacion) {
		this.empleadoidentificacion = empleadoidentificacion;
	}
	public Cargo getCargo() {
		return cargo;
	}
	public void setCargo(Cargo cargo) {
		this.cargo = cargo;
	}
	public Area getArea() {
		return area;
	}
	public void setArea(Area area) {
		this.area = area;
	}
	public HorarioAsignado getHorarioasignado() {
		return horarioasignado;
	}
	public void setHorarioasignado(HorarioAsignado horarioasignado) {
		this.horarioasignado = horarioasignado;
	}
	public Timestamp getHoraentradadiaria() {
		return horaentradadiaria;
	}
	public void setHoraentradadiaria(Timestamp horaentradadiaria) {
		this.horaentradadiaria = horaentradadiaria;
	}
	public Timestamp getHorasalidadiaria() {
		return horasalidadiaria;
	}
	public void setHorasalidadiaria(Timestamp horasalidadiaria) {
		this.horasalidadiaria = horasalidadiaria;
	}
	public List<HorarioparaReporte> getValores() {
		return valores;
	}
	public void setValores(List<HorarioparaReporte> valores) {
		this.valores = valores;
	}
	
	
	
	
	
	


}
