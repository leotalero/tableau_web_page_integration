package co.sistemcobro.rrhh.bean;

import java.io.Serializable;
import java.sql.Timestamp;

public class Disciplina implements Serializable {

	private static final long serialVersionUID = 1L;
	
	private Long iddisciplina;
	private Long idsancion;
	private Long idcontrato;
	private Integer idusuariocrea;
	private Timestamp fechacrea;
	private Integer idusuariomod;
	private Timestamp fechamod;
	private Integer estado;

	private Sancion sancion;
	private Falta falta;
	private FaltaTipo faltaTipo;
	private DisciplinaHistorial disciplinaHistorial;
	private DisciplinaEstado ultimoDisciplinaEstado;
	private String recurrenciaPorFalta;
	
	private Contrato contrato;
	private Area area;
	private EmpleadoBean empleado;
	
	public Long getIddisciplina() {
		return iddisciplina;
	}
	public void setIddisciplina(Long iddisciplina) {
		this.iddisciplina = iddisciplina;
	}
	public Long getIdsancion() {
		return idsancion;
	}
	public void setIdsancion(Long idsancion) {
		this.idsancion = idsancion;
	}
	public Long getIdcontrato() {
		return idcontrato;
	}
	public void setIdcontrato(Long idcontrato) {
		this.idcontrato = idcontrato;
	}
	public Integer getIdusuariocrea() {
		return idusuariocrea;
	}
	public void setIdusuariocrea(Integer idusuariocrea) {
		this.idusuariocrea = idusuariocrea;
	}
	public Timestamp getFechacrea() {
		return fechacrea;
	}
	public void setFechacrea(Timestamp fechacrea) {
		this.fechacrea = fechacrea;
	}
	public Integer getIdusuariomod() {
		return idusuariomod;
	}
	public void setIdusuariomod(Integer idusuariomod) {
		this.idusuariomod = idusuariomod;
	}
	public Timestamp getFechamod() {
		return fechamod;
	}
	public void setFechamod(Timestamp fechamod) {
		this.fechamod = fechamod;
	}
	public Integer getEstado() {
		return estado;
	}
	public void setEstado(Integer estado) {
		this.estado = estado;
	}
	public Sancion getSancion() {
		return sancion;
	}
	public void setSancion(Sancion sancion) {
		this.sancion = sancion;
	}
	public Falta getFalta() {
		return falta;
	}
	public void setFalta(Falta falta) {
		this.falta = falta;
	}
	public FaltaTipo getFaltaTipo() {
		return faltaTipo;
	}
	public void setFaltaTipo(FaltaTipo faltaTipo) {
		this.faltaTipo = faltaTipo;
	}
	public DisciplinaHistorial getDisciplinaHistorial() {
		return disciplinaHistorial;
	}
	public void setDisciplinaHistorial(DisciplinaHistorial disciplinaHistorial) {
		this.disciplinaHistorial = disciplinaHistorial;
	}
	public DisciplinaEstado getUltimoDisciplinaEstado() {
		return ultimoDisciplinaEstado;
	}
	public void setUltimoDisciplinaEstado(DisciplinaEstado ultimoDisciplinaEstado) {
		this.ultimoDisciplinaEstado = ultimoDisciplinaEstado;
	}
	public String getRecurrenciaPorFalta() {
		return recurrenciaPorFalta;
	}
	public void setRecurrenciaPorFalta(String recurrenciaPorFalta) {
		this.recurrenciaPorFalta = recurrenciaPorFalta;
	}
	public Contrato getContrato() {
		return contrato;
	}
	public void setContrato(Contrato contrato) {
		this.contrato = contrato;
	}
	public Area getArea() {
		return area;
	}
	public void setArea(Area area) {
		this.area = area;
	}
	public EmpleadoBean getEmpleado() {
		return empleado;
	}
	public void setEmpleado(EmpleadoBean empleado) {
		this.empleado = empleado;
	}
	
}
