package co.sistemcobro.rrhh.bean;

import java.io.Serializable;
import java.sql.Timestamp;
/**
 * 
 * @author Leonardo talero
 * 
 */
public class EmpleadoDocumentoPersonal implements Serializable {

	private static final long serialVersionUID = 1L;
	
	private Long idempleadodocumento;
	private Long idempleado;
	private Long iddocumentotipo;
	private String nombredocumento;
	private String rutadocumento;
	private Long tamaniodocumento;
	private String observaciondocumento;
	
	private Integer idusuariocrea;
	private Timestamp fechacrea;
	private Integer idusuariomod;
	private Timestamp fechamod;
	private Integer estado;
	
	private EmpleadoDocumentoTipo tipodocumento;
	
	
	public Long getIdempleadodocumento() {
		return idempleadodocumento;
	}
	public void setIdempleadodocumento(Long idempleadodocumento) {
		this.idempleadodocumento = idempleadodocumento;
	}
	public Long getIdempleado() {
		return idempleado;
	}
	public void setIdempleado(Long idempleado) {
		this.idempleado = idempleado;
	}
	public Long getIddocumentotipo() {
		return iddocumentotipo;
	}
	public void setIddocumentotipo(Long iddocumentotipo) {
		this.iddocumentotipo = iddocumentotipo;
	}
	public String getNombredocumento() {
		return nombredocumento;
	}
	public void setNombredocumento(String nombredocumento) {
		this.nombredocumento = nombredocumento;
	}
	public String getRutadocumento() {
		return rutadocumento;
	}
	public void setRutadocumento(String rutadocumento) {
		this.rutadocumento = rutadocumento;
	}
	public Long getTamaniodocumento() {
		return tamaniodocumento;
	}
	public void setTamaniodocumento(Long tamaniodocumento) {
		this.tamaniodocumento = tamaniodocumento;
	}
	public String getObservaciondocumento() {
		return observaciondocumento;
	}
	public void setObservaciondocumento(String observaciondocumento) {
		this.observaciondocumento = observaciondocumento;
	}
	public Integer getIdusuariocrea() {
		return idusuariocrea;
	}
	public void setIdusuariocrea(Integer idusuariocrea) {
		this.idusuariocrea = idusuariocrea;
	}
	public Timestamp getFechacrea() {
		return fechacrea;
	}
	public void setFechacrea(Timestamp fechacrea) {
		this.fechacrea = fechacrea;
	}
	public Integer getIdusuariomod() {
		return idusuariomod;
	}
	public void setIdusuariomod(Integer idusuariomod) {
		this.idusuariomod = idusuariomod;
	}
	public Timestamp getFechamod() {
		return fechamod;
	}
	public void setFechamod(Timestamp fechamod) {
		this.fechamod = fechamod;
	}
	public Integer getEstado() {
		return estado;
	}
	public void setEstado(Integer estado) {
		this.estado = estado;
	}
	
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	public EmpleadoDocumentoTipo getTipodocumento() {
		return tipodocumento;
	}
	public void setTipodocumento(EmpleadoDocumentoTipo tipodocumento) {
		this.tipodocumento = tipodocumento;
	}
	
}
