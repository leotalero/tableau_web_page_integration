package co.sistemcobro.altitude.bean;

/**
 * 
 * @author Jony Hurtado
 * 
 */
public class DialRuleBean {

	private Integer code;
	private String name;
	private Short recall_tries_total;
	private Short rec_tries_day_ttl;
	private Integer recall_next_dr;

	public Integer getCode() {
		return code;
	}

	public void setCode(Integer code) {
		this.code = code;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Short getRecall_tries_total() {
		return recall_tries_total;
	}

	public void setRecall_tries_total(Short recall_tries_total) {
		this.recall_tries_total = recall_tries_total;
	}

	public Short getRec_tries_day_ttl() {
		return rec_tries_day_ttl;
	}

	public void setRec_tries_day_ttl(Short rec_tries_day_ttl) {
		this.rec_tries_day_ttl = rec_tries_day_ttl;
	}

	public Integer getRecall_next_dr() {
		return recall_next_dr;
	}

	public void setRecall_next_dr(Integer recall_next_dr) {
		this.recall_next_dr = recall_next_dr;
	}

}
