package co.sistemcobro.all.exception;
/**
 * 
 * @author Jony Hurtado
 * 
 */
public class ProcesoException extends Exception {

	private static final long serialVersionUID = -7393514908352290012L;

	private String mensaje;

	public ProcesoException() {
	}

	public ProcesoException(String mensaje) {
		this.mensaje = mensaje;
	}

	@Override
	public String toString() {
		return mensaje;
	}

	@Override
	public String getMessage() {
		return mensaje;
	}
}
